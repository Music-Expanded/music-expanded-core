<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <RootNamespace>MusicExpanded</RootNamespace>
        <AssemblyName>MusicExpanded</AssemblyName>
        <OutputPath>Assemblies</OutputPath>
        <VersionPrefix>1.0.0.0</VersionPrefix>
        <OutputType>Library</OutputType>
        <TargetFramework>net472</TargetFramework>
        <PlatformTarget>x64</PlatformTarget>
        <DebugType>none</DebugType>
        <DebugSymbols>false</DebugSymbols>
        <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
        <Deterministic>false</Deterministic>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Krafs.Rimworld.Ref" Version="1.3.3326"/>
        <PackageReference Include="Lib.Harmony" Version="2.2.1">
            <ExcludeAssets>runtime</ExcludeAssets>
        </PackageReference>
        <PackageReference Include="UnlimitedHugs.Rimworld.HugsLib" Version="9.0.1">
            <ExcludeAssets>runtime</ExcludeAssets>
        </PackageReference>
    </ItemGroup>
</Project>