    <!-- Sounds -->
    <MusicExpanded.SoundDef>
        <defName>ME_Glitterworld_Scenario_Begin</defName>
        <replaces><li>GameStartSting</li></replaces>
        <subSounds>
            <li>
                <onCamera>True</onCamera>      
                <grains>
                    <li Class="AudioGrain_Clip">
                        <clipPath>MusicExpanded/Glitterworld/Glitterworld_Scenario_Begin</clipPath>
                    </li>
                </grains>      
                <volumeRange>100~100</volumeRange>
            </li>
        </subSounds>
    </MusicExpanded.SoundDef>
